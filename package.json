  "description": "TSLint rules for RxJS",
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2"
  },
  "homepage": "https://github.com/cartant/rxjs-tslint-rules",
  "keywords": [
